{
  "name": "demo",
  "version": "1.0.1",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "productName": "electron-demo",
    "appId": "org.electron-demo.electron-react-antd",
    "directories": {
      "output": "dist"
    },
    "publish": {
      "provider": "github",
      "owner": "xzifan",
      "repo": "awesome-electron-demo",
      "releaseType": "release"
    },
    "files": [
      "build/**/*"
    ]
  },
  "author": {
    "name": "zifan",
    "email": "xiao@zifan.dev",
    "url": "https://github.com/xzifan"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "create-react-app":"3.4.1",
    "antd": "^4.16.10",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sass-loader": "^12.1.0",
    "web-vitals": "^1.0.1",
    "xlsx": "^0.17.0"
  },
  "scripts": {
    "electron-dev": "electron .",
    "start": "react-scripts start",
    "build": "set CI=true && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist-win32": "electron-builder --win --ia32",
    "dist-win64": "electron-builder --win --x64",
    "dist-mac": "electron-builder --mac"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^13.1.8",
    "electron-builder": "^22.11.7"
  }
}
